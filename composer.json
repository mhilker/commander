{
    "name": "commander/commander",
    "description": "CQRS library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Maik Hilker",
            "email": "maikhilker89@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Commander\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Commander\\": "tests/"
        }
    }
}
